<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="./css/reset.css">
    <link rel="stylesheet" href="./css/style.css">
</head>
<body>
    <%- include ('../layout/header.ejs') %>
    <main>
        <form class="searchs" id="searchs">
            <input class="search" id="search" type="text" name="search" placeholder="ادخل ما تبحث عنه " >
            <button class="searchbutton">إبحث</button>
        </form>
       <%cdns.forEach((val)=>{ %>
        <div class="name">
           <h2 class="cdn"><%= val.name%></h2> 
           <p  class="cdn"><%=val.latest%></p>
           <p  class="cdn"><%=val.version%></p>
           <p  class="cdn"> <%=val.description%></p> 
           <p  class="cdn"><%=val.author%></p>
           <p  class="cdn"><%=val.filename%></p>
           <p  class="cdn"><%=val.keywords%></p>
           <button class="save">حفظ</button>
        </div>
            
       <%})%>
    </main>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.js"></script>
    <script src="./js/signup.js"></script>
</body>
</html>